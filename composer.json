{
    "require-dev": {
        "pestphp/pest": "^2.33",
        "symfony/var-dumper": "^7.0",
        "pestphp/pest-plugin-watch": "^2.1",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Domain\\Blog\\": "domain/Blog/src",
            "Domain\\Auth\\": "domain/Auth/src",
            "App\\": "infra"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Domain\\App\\Tests\\": "domain/App/tests",
            "Domain\\Blog\\Tests\\": "domain/Blog/tests",
            "Domain\\Auth\\Tests\\": "domain/Auth/tests"
        }
    },
    "require": {
        "beberlei/assert": "^3.3",
        "symfony/http-foundation": "^7.0",
        "php-di/php-di": "^7.0",
        "nikic/fast-route": "^1.3",
        "mnapoli/silly": "^1.9",
        "twig/twig": "^3.8",
        "altorouter/altorouter": "^2.0",
        "symfony/dotenv": "^7.0"
    }
}
